<?xml version="1.0" encoding="utf-8"?>
<!-- More info at: http://code.google.com/appengine/docs/java/datastore/queriesandindexes.html#Defining_Indexes_With_Configuration -->
<datastore-indexes autoGenerate="true">
<datastore-index kind="Item" ancestor="false" source="manual">
        <property name="active" direction="asc"/>
        <property name="dateCreated" direction="desc"/>
    </datastore-index>
    <datastore-index kind="Item" ancestor="false" source="manual">
        <property name="active" direction="asc"/>
        <property name="datePublished" direction="desc"/>
    </datastore-index>
     <datastore-index kind="Article" ancestor="false" source="manual">
        <property name="active" direction="asc"/>
        <property name="dateModified" direction="asc"/>
    </datastore-index>
    <datastore-index kind="Article" ancestor="false" source="manual">
        <property name="active" direction="asc"/>
        <property name="dateModified" direction="desc"/>
    </datastore-index>
    <datastore-index kind="Article" ancestor="false" source="manual">
        <property name="active" direction="asc"/>
        <property name="dateCreated" direction="desc"/>
    </datastore-index>
     <datastore-index kind="Category" ancestor="false" source="manual">
        <property name="parent" direction="asc"/>
        <property name="priority" direction="asc"/>
    </datastore-index>
 <datastore-index kind="X_CategoryItem" ancestor="false" source="manual">
        <property name="active" direction="asc"/>
        <property name="cat_slug" direction="asc"/>
        <property name="dateCreated" direction="desc"/>
    </datastore-index>
</datastore-indexes>